#/bin/bash

# This script uses simple workloads. You should use it as a test for your code

npages=100
clock=10

for alg in fifo nru lru aging second-chance belady
do
    for nframes in 4 8 16 32
    do
        for tlbsizes in 4 8 16 32
        do
	        python2.7 memsym.py $npages $nframes $alg $clock $tlbsizes < load/10000.100.random.r.in > output/10000.100.random.r.$alg.$nframes.out
        done
    done
done
