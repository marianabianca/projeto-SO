#/bin/bash

# This script exercises the real workload
npages=1500000
clock=200

for trace in workingset.trace
do
    for alg in fifo nru lru aging second-chance belady
    #for alg in fifo aging
    do
        for nframes in 4 8 16 32
        do
	    mkdir output/$trace
	    python2.7 memsym.py $npages $nframes $alg $clock < load/$trace.in > output/$trace/$trace.$alg.$nframes.out
	done
    done
done
