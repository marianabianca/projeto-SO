#/bin/bash

# This script exercises the real workload
npages=1500000
clock=200

for trace in trace.1 trace.2 workingset.trace
do
    for alg in fifo nru lru aging second-chance belady
    #for alg in fifo aging
    do
        for nframes in 4 8 16 32
        do
            for tlbsizes in 4 8 16 32
            do
            mkdir output/$trace
            python2.7 memsym.py $npages $nframes $alg $clock $tlbsizes < load/$trace.in > output/$trace/$trace.$alg.$nframes.$tlbsizes.out
            done
	    done
    done
done
